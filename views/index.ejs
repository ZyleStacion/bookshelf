<%- include ('partials/header', { title }) -%>

<div id="bookshelf" class="bookshelf">
    <% if (books.length > 0) { %>
        <% books.forEach(book => { %>     
            <section class = "book" id = "book">
                <p><%= book.cover %></p> 
            </section>
        <% }) %>
    <% } else { %>
        <p>No books available.</p>
    <% } %>
</div>

<%- include ('partials/footer') -%>